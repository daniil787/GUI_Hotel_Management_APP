<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.hotelmanagement.AccommodationController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label text="Phone Number"/>
    <TextField fx:id="phoneNumberField"/>

    <Button text="Check In" onAction="#onCheckInButtonClick"/>
    <Button text="Back" onAction="#backToReceptionistMenu"/>
</VBox>
